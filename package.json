{
  "name": "distapp",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "dev": "HOST=localhost HOST=0.0.0.0 nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "eslint --ext \".js,.vue\" --ignore-path .gitignore .",
    "lint:fix": "eslint --fix --ext \".js,.vue\" --ignore-path .gitignore .",
    "format": "prettier --write \"**/*.{js,vue,d.ts}\"",
    "format:check": "prettier --check \"**/*.{js,vue,d.ts}\"",
    "drizzle:generate": "drizzle-kit generate",
    "drizzle:migrate": "bun run server/db/migrate.ts",
    "deploy": "bun run build --preset=cloudflare-pages && npx wrangler pages deploy dist/",
    "e2e": "playwright test",
    "e2e-ui": "bun run e2e --ui-port=8080 --ui-host=127.0.0.1",
    "tests": "vitest test",
    "build-cli": "bun build cli/cli.ts --target node > cli/cli.mjs",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "cli:pack": "bun run build-cli && cd cli && bun pm pack",
    "cli:publish": "cd cli && bun publish"
  },
  "dependencies": {
    "@hebilicious/vue-query-nuxt": "^0.3.0",
    "@lemonsqueezy/lemonsqueezy.js": "^4.0.0",
    "@libsql/client": "^0.14.0",
    "@plist/parse": "^1.1.0",
    "@polar-sh/adapter-utils": "^0.2.0",
    "@polar-sh/sdk": "^0.34.2",
    "@primevue/nuxt-module": "4.0.2",
    "@primevue/themes": "4.0.2",
    "@tanstack/vue-query": "^5.28.9",
    "@vueuse/core": "^10.10.0",
    "@vueuse/nuxt": "^10.10.0",
    "@xmldom/xmldom": "^0.8.10",
    "aws4fetch": "^1.0.19",
    "caniuse-lite": "^1.0.30001679",
    "chart.js": "3.3.2",
    "crypto-js": "^4.2.0",
    "drizzle-orm": "^0.36.0",
    "es-toolkit": "^1.34.1",
    "fast-xml-parser": "^4.4.1",
    "h3": "^1.11.1",
    "jose": "^5.2.4",
    "jszip": "^3.10.1",
    "pg": "^8.11.3",
    "primeicons": "^7.0.0",
    "primevue": "4.0.2",
    "simple-xml-to-json": "^1.2.3",
    "slugify": "^1.6.6",
    "uuidv7": "^1.0.1",
    "vitest": "^2.1.2",
    "vue3-google-signin": "^2.0.0",
    "winston": "^3.15.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.18.9",
    "@nuxt/test-utils": "^3.15.4",
    "@nuxtjs/google-fonts": "^3.2.0",
    "@playwright/test": "1.48.0",
    "@types/crypto-js": "^4.2.2",
    "@types/pg": "^8.11.4",
    "@vue/test-utils": "^2.4.6",
    "autoprefixer": "^10.4.20",
    "bun-types": "^1.1.30",
    "dotenv": "^16.4.5",
    "drizzle-kit": "^0.27.1",
    "eslint": "^8.30.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-nuxt": "^4.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.4.0",
    "happy-dom": "^15.7.4",
    "nuxt": "^3.15.2",
    "nuxt-svgo-loader": "^0.4.1",
    "playwright-core": "1.48.0",
    "postcss": "^8.4.48",
    "prettier": "2.7.1",
    "sass": "^1.76.0",
    "sass-loader": "^10.4.1",
    "tailwindcss": "^3.4.3",
    "vite-svg-loader": "^5.1.0",
    "vitepress": "^1.6.3"
  },
  "trustedDependencies": [
    "@hebilicious/vue-query-nuxt",
    "esbuild",
    "vue-demi"
  ]
}